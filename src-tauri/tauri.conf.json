{
	"productName": "TelevyBackup",
	"identifier": "com.ivan.televybackup",
	"version": "0.1.0",
	"build": {
		"beforeDevCommand": "bun run dev",
		"beforeBuildCommand": "bun run build",
		"devUrl": "http://localhost:51731",
		"frontendDist": "../web/dist"
	},
	"app": {
		"security": {
			"csp": null
		},
		"windows": [
			{
				"label": "main",
				"title": "TelevyBackup",
				"width": 1024,
				"height": 768,
				"resizable": true,
				"fullscreen": false
			}
		]
	},
	"bundle": {
		"icon": ["icons/icon.png"]
	},
	"plugins": {}
}
